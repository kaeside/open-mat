{
  "name": "open-mat",
  "version": "1.0.0",
  "description": "",
  "main": "client/index.jsx",
  "scripts": {
    "build": "npm run clean && npm run mkdir && npm run build:html && npm run build:jsx", 
    "build:prod": "npm run clean && npm run mkdir && npm run build:html && webpack -p",
    "watch": "npm run watch:html & npm run watch:jsx",
    "test": "mocha --compilers js:babel-register test/**/*.js*",
    "serve": "http-server build",
    "mkdir": "mkdir -p build",
    "clean": "rm -rf build",
    "build:html": "npm run clean:html && cp client/index.html build/",
    "watch:html": "npm run build:html && chokidar client/index.html -c 'npm run build:html'",
    "clean:html": "rm -f build/index.html",
    "build:js": "npm run clean:js && webpack",
    "watch:js": "npm run build:js && webpack --watch",
    "clean:js": "rm -f build/$npm_package_name.$npm_package_version.js build/$npm_package_name.$npm_package_version.js.map",
    "clean:css": "rm build/css/main.css",
    "build:jsx": "npm run clean:jsx && webpack",
    "watch:jsx": "npm run build:jsx && webpack --watch",
    "clean:jsx": "rm -f build/$npm_package_name.$npm_package_version.jsx build/$npm_package_name.$npm_package_version.jsx.map"
  },
  "author": "Kyle Ludlow",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": ">=6.8.0",
    "babel-loader": ">=6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": ">=6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": ">=6.8.0",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "chokidar-cli": "^1.2.0",
    "css-loader": ">=0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "http-server": "^0.9.0",
    "image-loader": "0.0.1",
    "img-loader": "^1.3.1",
    "mocha": ">=2.4.5",
    "node-sass": "^3.8.0",
    "nodemon": ">=1.9.2",
    "react-addons-test-utils": ">=15.0.2",
    "sass-loader": "^4.0.0",
    "url-loader": "^0.5.7",
    "webpack": ">=1.13.0",
    "webpack-dev-server": ">=1.14.1"
  },
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "babel": "^6.5.2",
    "body-parser": ">=1.15.1",
    "chai": "^3.5.0",
    "express": "^4.13.4",
    "google-map-react": "^0.15.1",
    "isomorphic-fetch": ">=2.2.1",
    "mocha": "^2.5.3",
    "mongodb": "^2.1.19",
    "mongoose": "^4.4.19",
    "react": ">=15.1.0",
    "react-addons-update": ">=15.0.2",
    "react-dom": ">=15.0.2",
    "react-google-maps": "^4.11.0",
    "react-redux": ">=4.4.5",
    "react-router": "^2.4.1",
    "redux": ">=3.5.2",
    "redux-thunk": ">=2.1.0"
  },
  "source": [
    "client/**/*.js",
    "server/**/*.js"
  ]
}
